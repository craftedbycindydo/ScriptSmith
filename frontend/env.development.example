# Frontend Development Environment Configuration
# Copy this file to .env.local for local development (Vite uses .env.local)
# ALL VALUES ARE REQUIRED - NO FALLBACKS PROVIDED

# API Configuration - REQUIRED
VITE_API_BASE_URL=http://localhost:8082/api
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Online IDE Platform
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_LOGS=true

# Code Editor Configuration
VITE_DEFAULT_LANGUAGE=python
VITE_AUTO_SAVE_INTERVAL=5000
VITE_CODE_EXECUTION_TIMEOUT=30000

# UI Configuration
VITE_THEME=light
VITE_ENABLE_DARK_MODE=true

# Security Configuration
VITE_ENABLE_CSP=false
VITE_SECURE_COOKIES=false
